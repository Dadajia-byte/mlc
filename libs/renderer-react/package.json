{
  "name": "@mlc/renderer-react",
  "version": "1.0.0",
  "type": "module",
  "description": "React 渲染器 - 用于 React 环境渲染低代码组件",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "../../node_modules/.bin/tsc && vite build",
    "type-check": "../../node_modules/.bin/tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "@mlc/renderer-core": "workspace:*",
    "@mlc/schema": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  }
}
