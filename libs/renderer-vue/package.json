{
  "name": "@mlc/renderer-vue",
  "version": "1.0.0",
  "type": "module",
  "description": "Vue 渲染器 - 用于 Vue 环境渲染低代码组件",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "type-check": "vue-tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "@mlc/renderer-core": "workspace:*",
    "@mlc/schema": "workspace:*"
  },
  "peerDependencies": {
    "vue": ">=3.3"
  },
  "devDependencies": {
    "vue-tsc": "^1.8.0"
  }
}
